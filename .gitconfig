[user]
	email = contant@mnaoki72.sakura.ne.jp
	name = nminamid
[push]
	default = simple
[alias]
    st = status -s -- ./
	sts = status -s
	co = checkout
	ci = commit -v
	cim = commit -v --amend
	recommit = commit --amend -C HEAD -q
	br = branch

	rewind = reset --hard
	unstage = reset HEAD -- 
	uncommit = reset --soft HEAD^

	# subversion
	ish = svn find-rev
[color]
	auto
[core]
	editor = vim
